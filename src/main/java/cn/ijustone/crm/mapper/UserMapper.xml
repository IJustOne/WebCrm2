<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.ijustone.crm.mapper.UserMapper">

       
	
	<!-- 通过id获取 -->
	<select id="queryOne" resultType="cn.ijustone.crm.domain.User" parameterType="long">
		select
		id,username,password,age
		from t_user where id=#{id}
	</select>



</mapper> 